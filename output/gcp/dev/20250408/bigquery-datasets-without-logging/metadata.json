{
  "policy": {
    "name": "bigquery-datasets-without-logging",
    "resource": "gcp.bq-dataset",
    "description": "Identifies BigQuery datasets older than 30 days that do not have \ndata access logging enabled. Access logs are essential for monitoring \nusage patterns and security compliance.\n",
    "filters": [
      {
        "type": "value",
        "key": "creationTime",
        "value_type": "age",
        "op": "greater-than",
        "value": 30
      },
      {
        "not": [
          {
            "type": "value",
            "key": "datasetReference.datasetId",
            "op": "regex",
            "value": "^(temp|test).*"
          }
        ]
      },
      {
        "type": "value",
        "key": "access_logs.logBigqueryAuditMetadata",
        "value": false
      }
    ],
    "actions": [
      {
        "type": "notify",
        "to": [
          "dataops@example.com"
        ],
        "format": "json",
        "subject": "BigQuery Dataset without Access Logging: {datasetReference.datasetId}",
        "transport": {
          "type": "pubsub",
          "topic": "projects/{project_id}/topics/custodian-notifications"
        }
      }
    ]
  },
